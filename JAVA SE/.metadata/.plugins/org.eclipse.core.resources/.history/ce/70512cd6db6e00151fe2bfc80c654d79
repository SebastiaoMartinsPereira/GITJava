import java.awt.List;import java.util.ArrayList;


public class Baralho {

	private ArrayList<Carta> cartasFora;
	private ArrayList<Carta> cartasNoBaralho;
	private ArrayList<tipoFace> faces = new ArrayList<tipoFace>();
	private ArrayList<tipoNaipe> naipes = new ArrayList<tipoNaipe>();
	
	
	public ArrayList<tipoFace> getFaces() {
		return faces;
	}
	public void setFaces(ArrayList<tipoFace> faces) {
		this.faces = faces;
	}

	
	
}
