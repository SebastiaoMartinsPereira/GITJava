package newprogsoftwares.estruturadedados.fj14;

public class TesteArray {
	
	public static void main(String[] args) {
		
		Aluno a1 = new Aluno("aluno1", 1510171, 26);
		Aluno a2 = new Aluno("aluno2", 1710171, 30);
		Aluno a3 = new Aluno("aluno3", 1710171, 30);
		Aluno a4 = new Aluno("aluno4", 1710171, 30);
		Aluno a5 = new Aluno("aluno5", 1710171, 30);
		Fj14_Array lista = new Fj14_Array();
		
        //adiciona no fim da lista.
		lista.adiciona(a1);
		lista.adiciona(a2);
		lista.adiciona(a3);
		lista.adiciona(a4);
		lista.adiciona(a5);
		
		System.out.println(lista);
		
		
		
	}
}
